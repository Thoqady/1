#include <iostream>
#include <map>


using namespace std;

int main()
{
    int x,y;
    cout << "Vvedite x ";
    cin >> x;
    cout << "Vvedite y ";
    cin >> y;
    int a = x + y;
    int b = x - y;
    cout << "Resultat slojeniya " << a << endl;
    cout << "Resultat vichitaniya " << b << endl;
     return 0;
}
